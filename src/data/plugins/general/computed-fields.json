{
  "fields": [
    {
      "id": "avgFloorHeight",
      "description": "Altura média por piso (buildingHeight / numberOfFloors)",
      "computation": {
        "type": "arithmetic",
        "operands": ["buildingHeight", "numberOfFloors"],
        "operation": "divide"
      }
    },
    {
      "id": "windowToFloorRatio",
      "description": "Rácio entre área de vãos e área útil (envelope.windowArea / usableFloorArea)",
      "computation": {
        "type": "arithmetic",
        "operands": ["envelope.windowArea", "usableFloorArea"],
        "operation": "divide"
      }
    }
  ]
}
